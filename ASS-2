#include<stdio.h>
int main()
{
	int a[100],n,i,j,c=0;
	printf("Enter the size of array:");
	scanf("%d",&n);
	printf("Enter the elements of array:");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("Enter the input:");
	scanf("%d",&j);
	for(i=0;i<n;i++)
	{
		if(j==a[i])
		c++;
	}
	printf("Frequency of the given input is: %d",c);
}
